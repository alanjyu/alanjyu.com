{
  "name": "alanjyu.com",
  "version": "1.7",
  "default": "src/js/main.js",
  "description": "Alan Yu's web portfolio.",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "parcel serve ./src/index.html ./src/research/index.html ./src/cv/index.html ./src/hobbies/index.html",
    "build": "parcel build ./src/index.html ./src/research/index.html ./src/cv/index.html ./src/hobbies/index.html --dist-dir docs/",
    "publish": "parcel build ./src/index.html ./src/research/index.html ./src/cv/index.html ./src/hobbies/index.html --public-url https://alanjyu.com --dist-dir docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanjyu/alanjyu.com.git"
  },
  "author": "Alan J. Yu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alanjyu/alanjyu.com/issues"
  },
  "homepage": "https://github.com/alanjyu/",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@parcel/transformer-webmanifest": "^2.12.0",
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "lmdb": "^2.8.2"
  }
}
